<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" mc:Ignorable="d" xmlns:local="clr-namespace:Egs"
        xmlns:dotNetUtilViews="clr-namespace:Egs.DotNetUtility.Views">

    <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
    <dotNetUtilViews:TrueToCollapsedFalseToVisibleConverter x:Key="TrueToCollapsedFalseToVisibleConverter" />
    <dotNetUtilViews:BoolToOppositeBoolConverter x:Key="BoolToOppositeBoolConverter" />
    <dotNetUtilViews:EnumToBooleanConverter x:Key="EnumToBooleanConverter" />
    <dotNetUtilViews:EnumToVisibilityConverter x:Key="EnumToVisibilityConverter" />
    <dotNetUtilViews:Int32IndexToBooleanConverter x:Key="Int32IndexToBooleanConverter" />

    <Style x:Key="LauncherButtonStyle" TargetType="{x:Type Button}">
        <Setter Property="Foreground" Value="White" />
        <Setter Property="FontSize" Value="14.5" />
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="TextBlock.TextAlignment" Value="Center" />
        <Setter Property="Width" Value="116" />
        <Setter Property="Height" Value="116" />
        <Setter Property="Margin" Value="0" />
        <Setter Property="Content" Value="{Binding ButtonContentText}" />
        <Setter Property="Command" Value="{Binding Command}" />
        <Setter Property="Margin" Value="2" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid>
                        <Ellipse x:Name="OuterBorderShape" Width="106" Height="106">
                            <Ellipse.Fill>
                                <SolidColorBrush Color="Black" />
                            </Ellipse.Fill>
                        </Ellipse>
                        <Ellipse x:Name="OuterShape" Width="104" Height="104">
                            <Ellipse.Fill>
                                <SolidColorBrush x:Name="ShapeEdgeFillBrush" Color="White" />
                            </Ellipse.Fill>
                        </Ellipse>
                        <Ellipse x:Name="InnerShape" Width="98" Height="98">
                            <Ellipse.Fill>
                                <SolidColorBrush x:Name="ShapeInsideFillBrush" Color="Black" />
                            </Ellipse.Fill>
                        </Ellipse>
                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Trigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation Storyboard.TargetName="ShapeInsideFillBrush"
                                                Storyboard.TargetProperty="Color" To="RoyalBlue" Duration="0:0:0.1" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation Storyboard.TargetName="ShapeInsideFillBrush"
                                                Storyboard.TargetProperty="Color" To="Black" Duration="0:0:0.05" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="OuterBorderShape" Property="Width" Value="102" />
                            <Setter TargetName="OuterBorderShape" Property="Height" Value="102" />
                            <Setter TargetName="OuterShape" Property="Width" Value="100" />
                            <Setter TargetName="OuterShape" Property="Height" Value="100" />
                            <Setter TargetName="InnerShape" Property="Width" Value="92" />
                            <Setter TargetName="InnerShape" Property="Height" Value="92" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="LauncherSmallButtonStyle" TargetType="{x:Type Button}">
        <Setter Property="Foreground" Value="White" />
        <Setter Property="Width" Value="92" />
        <Setter Property="Height" Value="92" />
        <Setter Property="Margin" Value="0" />
        <Setter Property="FontSize" Value="14.5" />
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="TextBlock.TextAlignment" Value="Center" />
        <Setter Property="Content" Value="{Binding ButtonContentText}" />
        <Setter Property="Command" Value="{Binding Command}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid>
                        <Ellipse x:Name="OuterBorderShape" Width="84" Height="84" Fill="Black" />
                        <Ellipse x:Name="OuterShape" Width="82" Height="82" Stroke="White" StrokeThickness="3">
                            <!--<Ellipse.Fill>
                                    <SolidColorBrush Color="OrangeRed" Opacity="{Binding IsSelectedOpacity}" />
                                </Ellipse.Fill>-->
                        </Ellipse>
                        <Ellipse x:Name="IsMouseOverFillBrushShape" Width="76" Height="76">
                            <Ellipse.Fill>
                                <SolidColorBrush x:Name="IsMouseOverFillBrush" Color="RoyalBlue" Opacity="0" />
                            </Ellipse.Fill>
                        </Ellipse>
                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Trigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="IsMouseOverFillBrush"
                                                Storyboard.TargetProperty="Opacity" To="1.0" Duration="0:0:0.1" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="IsMouseOverFillBrush"
                                                Storyboard.TargetProperty="Opacity" To="0.0" Duration="0:0:0.05" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="OuterBorderShape" Property="Width" Value="80" />
                            <Setter TargetName="OuterBorderShape" Property="Height" Value="80" />
                            <Setter TargetName="OuterShape" Property="Width" Value="78" />
                            <Setter TargetName="OuterShape" Property="Height" Value="78" />
                            <Setter TargetName="IsMouseOverFillBrushShape" Property="Width" Value="70" />
                            <Setter TargetName="IsMouseOverFillBrushShape" Property="Height" Value="70" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="LauncherLaunchableApplicationButtonStyle" TargetType="{x:Type Button}">
        <Setter Property="Foreground" Value="White" />
        <Setter Property="FontSize" Value="14.5" />
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="TextBlock.TextAlignment" Value="Center" />
        <Setter Property="Command" Value="{Binding LaunchingOtherApplicationButtonModel.Command}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid x:Name="OuterBorderPanel" Width="120" MinHeight="60">
                        <Grid.Background>
                            <SolidColorBrush x:Name="panelBackgroundBrush" Color="Black" />
                        </Grid.Background>
                        <StackPanel VerticalAlignment="Center">
                            <Image Source="{Binding AppIconImageSource}" />
                            <TextBlock Text="{Binding AppTitle}" />
                        </StackPanel>
                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Trigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation Storyboard.TargetName="panelBackgroundBrush"
                                                Storyboard.TargetProperty="Color" To="RoyalBlue" Duration="0:0:0.1" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation Storyboard.TargetName="panelBackgroundBrush"
                                                Storyboard.TargetProperty="Color" To="Black" Duration="0:0:0.05" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="OuterBorderPanel" Property="Width" Value="118" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <ControlTemplate x:Key="cameraViewGlassButtonTemplate" TargetType="{x:Type Button}">
        <ControlTemplate.Resources>
            <SolidColorBrush x:Key="GlassButtonBorder01BorderBrush" Color="#FF00FFFF" />
            <SolidColorBrush x:Key="GlassButtonBorder02BorderBrush" Color="#FF007FFF" />
            <SolidColorBrush x:Key="GlassButtonBorder02Background" Color="#7F003FFF" />
            <SolidColorBrush x:Key="GlassButtonBorder02BackgroundMouseDown" Color="#7F003FFF" />
            <Storyboard x:Key="ButtonEnterActionTimeline">
                <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="glow"
                        Storyboard.TargetProperty="(UIElement.Opacity)">
                    <SplineDoubleKeyFrame KeyTime="00:00:00.100" Value="1" />
                </DoubleAnimationUsingKeyFrames>
            </Storyboard>
            <Storyboard x:Key="ButtonExitActionTimeline">
                <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="glow"
                        Storyboard.TargetProperty="(UIElement.Opacity)">
                    <SplineDoubleKeyFrame KeyTime="00:00:00.200" Value="0" />
                </DoubleAnimationUsingKeyFrames>
            </Storyboard>
        </ControlTemplate.Resources>
        <Border BorderBrush="{StaticResource GlassButtonBorder01BorderBrush}" BorderThickness="0" CornerRadius="10">
            <Border x:Name="border" BorderBrush="{StaticResource GlassButtonBorder02BorderBrush}"
                    Background="{StaticResource GlassButtonBorder02Background}" BorderThickness="3" CornerRadius="8.5">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="0.507*" />
                        <RowDefinition Height="0.493*" />
                    </Grid.RowDefinitions>
                    <Border Opacity="0" HorizontalAlignment="Stretch" x:Name="glow" Width="Auto" Grid.RowSpan="2"
                            CornerRadius="7">
                        <Border.Background>
                            <RadialGradientBrush>
                                <RadialGradientBrush.RelativeTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="1.702" ScaleY="2.243" />
                                        <SkewTransform AngleX="0" AngleY="0" />
                                        <RotateTransform Angle="0" />
                                        <TranslateTransform X="-0.368" Y="-0.152" />
                                    </TransformGroup>
                                </RadialGradientBrush.RelativeTransform>
                                <GradientStop Color="#B28DBDFF" Offset="0" />
                                <GradientStop Color="#008DBDFF" Offset="1" />
                            </RadialGradientBrush>
                        </Border.Background>
                    </Border>
                    <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" Width="Auto"
                            Grid.RowSpan="2" />
                    <Border HorizontalAlignment="Stretch" Margin="0" x:Name="shine" Width="Auto" CornerRadius="7,7,0,0">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.494,0.889" StartPoint="0.494,0.028">
                                <GradientStop Color="#99FFFFFF" Offset="0" />
                                <GradientStop Color="#33FFFFFF" Offset="1" />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Grid>
            </Border>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Opacity" TargetName="shine" Value="0.4" />
                <Setter Property="Background" TargetName="border"
                        Value="{StaticResource GlassButtonBorder02BackgroundMouseDown}" />
                <Setter Property="Visibility" TargetName="glow" Value="Hidden" />
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Trigger.EnterActions>
                    <BeginStoryboard Storyboard="{StaticResource ButtonEnterActionTimeline}" />
                </Trigger.EnterActions>
                <Trigger.ExitActions>
                    <BeginStoryboard Storyboard="{StaticResource ButtonExitActionTimeline}" />
                </Trigger.ExitActions>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <Style TargetType="GroupBox">
        <Setter Property="HeaderTemplate">
            <Setter.Value>
                <DataTemplate>
                    <TextBlock Text="{Binding}" Foreground="RoyalBlue" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Margin" Value="1" />
    </Style>
    <Style TargetType="CheckBox">
        <Setter Property="Margin" Value="10" />
        <Setter Property="MinHeight" Value="20" />
    </Style>
    <Style TargetType="Label">
        <Setter Property="MinHeight" Value="20" />
    </Style>
    <Style TargetType="ComboBox">
        <Setter Property="MinHeight" Value="20" />
    </Style>
    <Style TargetType="Expander">
        <Setter Property="HeaderTemplate">
            <Setter.Value>
                <DataTemplate>
                    <TextBlock Text="{Binding}" Foreground="RoyalBlue" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Margin" Value="1" />
    </Style>

    <Style x:Key="listBoxWithRadioButton" TargetType="{x:Type ListBox}">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="ItemTemplate">
            <Setter.Value>
                <DataTemplate>
                    <RadioButton Content="{TemplateBinding Content}" Margin="5,10" VerticalContentAlignment="Center"
                            IsChecked="{Binding Path=IsSelected, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type ListBoxItem}}}" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="CursorImageSetInformationOptionalValueListBoxWithRadioButton" TargetType="{x:Type ListBox}">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="ItemTemplate">
            <Setter.Value>
                <DataTemplate>
                    <RadioButton Margin="5,10" VerticalContentAlignment="Center"
                            IsChecked="{Binding Path=IsSelected, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type ListBoxItem}}}">
                        <Image Source="{Binding SampleImageSource}" />
                    </RadioButton>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <ItemsPanelTemplate x:Key="ItemsPanelTemplateIsWrapPanel">
        <WrapPanel />
    </ItemsPanelTemplate>
    <ItemsPanelTemplate x:Key="ItemsPanelTemplateIsVerticalStackPanel">
        <StackPanel Orientation="Vertical" />
    </ItemsPanelTemplate>
    <ItemsPanelTemplate x:Key="ItemsPanelTemplateIsUniformGrid">
        <UniformGrid />
    </ItemsPanelTemplate>
    <ItemsPanelTemplate x:Key="ItemsPanelTemplateIsTwoColumnsUniformGrid">
        <UniformGrid Columns="2" />
    </ItemsPanelTemplate>
    <ItemsPanelTemplate x:Key="ItemsPanelTemplateIsFiveColumnsUniformGrid">
        <UniformGrid Columns="5" />
    </ItemsPanelTemplate>
    <ItemsPanelTemplate x:Key="ItemsPanelTemplateIsSixColumnsUniformGrid">
        <UniformGrid Columns="6" />
    </ItemsPanelTemplate>

    <Storyboard x:Key="SetVisibilityToCollapsedWithOpacityDecreaseStoryboardKey">
        <DoubleAnimation Storyboard.TargetProperty="Opacity" Duration="0:0:0.200" From="1.0" To="0.0" />
        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Visibility">
            <DiscreteObjectKeyFrame KeyTime="0:0:0.200" Value="{x:Static Visibility.Collapsed}" />
        </ObjectAnimationUsingKeyFrames>
    </Storyboard>

    <Storyboard x:Key="SetVisibilityToVisibleWithOpacityIncreaseStoryboardKey">
        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Visibility">
            <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{x:Static Visibility.Visible}" />
        </ObjectAnimationUsingKeyFrames>
        <DoubleAnimation Storyboard.TargetProperty="Opacity" Duration="0:0:0.050" From="0.0" To="1.0" />
    </Storyboard>

    <Storyboard x:Key="SetWindowStateToMinimizedWithOpacityDecreaseStoryboardKey">
        <DoubleAnimation Storyboard.TargetProperty="Opacity" Duration="0:0:0.200" From="1.0" To="0.0" />
        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="WindowState">
            <DiscreteObjectKeyFrame KeyTime="0:0:0.200" Value="{x:Static WindowState.Minimized}" />
        </ObjectAnimationUsingKeyFrames>
    </Storyboard>

    <Storyboard x:Key="SetWindowStateToNormalWithOpacityIncreaseStoryboardKey">
        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="WindowState">
            <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{x:Static WindowState.Normal}" />
        </ObjectAnimationUsingKeyFrames>
        <DoubleAnimation Storyboard.TargetProperty="Opacity" Duration="0:0:0.050" From="0.0" To="1.0" />
    </Storyboard>

